<script type="text/javascript">
    $(document).ready(function () {

        if ($("#language").html() == "اللغة") {
            $('select').select2({ dir: "rtl" });
            $("#DivButton").css('float', 'left')
        }
        else {
            $('select').select2();
            $("#DivButton").css('float', 'right')
            $("#ace-settings-rtl").click();
            document.getElementById("ace-settings-rtl").checked = true;
        }
        //to make side bar open as the last time befor load the new page
        var elementID = Number(sessionStorage.getItem("elementID"));
        var element = $("#" + elementID)[0];
        var parent;
        while (element != undefined && element != null) {

            //to stop loop from going to top of side bar because this causes an issues in side bar
            if (element.classList == "nav nav-list")
                break;

            // this class to make the display as block
            $(element).addClass("ShowMyLi");

            //get the parent of the current element
            parent = element.parentElement;
            element = parent;
        }
        sessionStorage.setItem("elementID", 0);

    })

